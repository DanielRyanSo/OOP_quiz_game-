@page
@model OOP_Proj.Pages.RegisterModel
<h2>Register</h2>

<form method="post">
    <input asp-for="Username" placeholder="Username" required /><br />
    <input asp-for="Password" type="password" placeholder="Password" required /><br />
    <button type="submit">Register</button>
</form>

<p>@Model.Message</p>
<a href="/Login">Login</a>
